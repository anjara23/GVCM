<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="bean.Patient" table="PATIENT">
        <id name="codepat" type="int">
            <column name="codepat" />
            <generator class="increment"/>
        </id>
        
        <property name="nom" type="java.lang.String">
            <column name="nom_patient" />
        </property>
        
        <property name="prenom" type="java.lang.String">
            <column name="prenom_patient" />
        </property>
        
        <property name="sexe" type="java.lang.String">
            <column name="sexe_patient" />
        </property>
        
        <property name="adresse" type="java.lang.String">
            <column name="adresse_patient" />
        </property>
        
    </class>
</hibernate-mapping>


